<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TerceiraActivity">

    <TextView
        android:id="@+id/infoApp"
        android:layout_width="365dp"
        android:layout_height="210dp"
        android:layout_marginTop="32dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:text="@string/info_app1"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/infoApp2"
        android:layout_width="368dp"
        android:layout_height="170dp"
        android:layout_marginTop="4dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:text="@string/info_app2"
        app:layout_constraintEnd_toEndOf="@+id/infoApp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/infoApp"
        app:layout_constraintTop_toBottomOf="@+id/infoApp"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="364dp"
        android:layout_height="198dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:text="@string/info_app3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/infoApp2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/infoApp2"
        app:layout_constraintTop_toBottomOf="@+id/infoApp2"
        app:layout_constraintVertical_bias="0.088"
        tools:ignore="TextSizeCheck" />
</androidx.constraintlayout.widget.ConstraintLayout>